<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="common_background_drawable">
        <attr name="stateful" format="boolean"/>
        <attr name="shape">
            <enum name="rect" value="0"/>
            <enum name="round_rect" value="1"/>
            <enum name="side_circle_rect" value="2"/>
            <enum name="circle" value="3"/>
        </attr>
        <attr name="fill_mode">
            <flag name="solid" value="1"/>
            <flag name="bitmap" value="2"/>
        </attr>
        <attr name="scale_type">
            <enum name="none" value="0"/>
            <enum name="at_most" value="1"/>
            <enum name="at_least" value="2"/>
            <enum name="fit_frame" value="3"/>
        </attr>
        <attr name="stroke_mode">
            <enum name="none" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="dash" value="2"/>
        </attr>
        <attr name="radius" format="dimension"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="stroke_dash_solid" format="dimension"/>
        <attr name="stroke_dash_space" format="dimension"/>
        <attr name="color_disabled" format="color"/>
        <attr name="color_normal" format="color"/>
        <attr name="color_pressed" format="color"/>
        <attr name="color_stroke" format="color"/>
        <attr name="bitmap" format="reference"/>
    </declare-styleable>
</resources>